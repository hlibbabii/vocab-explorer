{
	"name": "vocab-explorer",
	"displayName": "vocab-explorer",
	"description": "desc",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:java",
		"onView:vocabulary"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "vocabulary",
					"name": "Vocabulary",
					"when": "true",
					"icon": "media/vocab.svg",
					"contextualTitle": "Vocabulary"
				}
			]
		}, "commands": [
			{
                "command": "vocab-explorer.refreshEntry",
                "title": "Refresh1",
                "icon": {
                    "light": "resources/media/refresh.svg",
                    "dark": "resources/media/refresh.svg"
                }
            }
		], "menus": {"view/title": [
			{
				"command": "vocab-explorer.refreshEntry",
				"when": "view == vocabulary",
				"group": "navigation"
			}
  ]}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"js-tokens": "^6.0.0",
		"yarn": "^1.22.5"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}
